node {
   def commit_id
   stage('Preparation') {
     checkout scm
     sh "git rev-parse --short HEAD > .git/commit-id"
     commit_id = readFile('.git/commit-id').trim()
   } 
   
   stage('test with licensefinder') {
     docker.image("licensefinder/license_finder:latest").inside('-v ${PWD}:/scan'){
         sh 'echo ${PWD}'
         sh 'ls'
         sh 'cd ..'
         sh 'ls'
         sh 'license_finder'
     }
   } 
   
                                      
}                   
