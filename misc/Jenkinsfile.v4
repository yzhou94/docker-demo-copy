pipeline {
    agent none
    stages {
        
        stage('test licensefinder') {
            agent {
                docker { 
                        image 'licensefinder/license_finder:latest'                        
                }
            }
            steps {
                sh 'echo $PWD'
                sh 'cd ..'
                sh 'ls'
                sh '/bin/bash -login'
                sh 'license_finder -p'
            }
            
        }
    }
}
